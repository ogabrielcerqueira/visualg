algoritmo "teste_enquanto"

var

    x, soma : inteiro

inicio

      escreva("Digite o primeiro numero: ")
      leia(x)
      
      soma <- 0
      enquanto x <> 0 faca
      soma <- soma + x
           escreva("Digite outro numero: ")
           leia(x)
      fimenquanto
      
      escreval("SOMA = ", soma)
   

fimalgoritmo
